{
  "name": "visualizer.js",
  "version": "2.0.0",
  "description": "An HTML5 audio visualizer for mic/line-in input",
  "main": "index.js",
  "scripts": {
    "build": "browserify index.js --standalone visualizer --outfile visualizer.browser.js && npm run compress",
    "compress": "uglifyjs visualizer.browser.js --compress --mangle > visualizer.browser.min.js",
    "watch": "watchify index.js -v -o visualizer.browser.js --verbose --standalone visualizer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mreinstein/visualizer.git"
  },
  "author": "Ian Harmon",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^14.0.0",
    "uglify-es": "^3.0.3",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "clamp": "^1.0.1",
    "get-user-media-promise": "^1.1.1",
    "next-tick-2": "^1.1.0",
    "raf": "^3.3.0"
  }
}
